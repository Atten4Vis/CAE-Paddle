{
    "architecture":{
        "image_module":{
            "name":"V2Net",
            "out_channels":128
        },
        "task_module":{
            "num_labels":16
        }
    }
    "metric":[
        {
            "name":"rvlcdip_acc",
            "type":"ClsMetric"
        }
    ],
    "eval":{
        "feed_names":[
            "image",
            "label"
        ],
        "loader":{
            "collect_batch":true,
            "num_workers":5,
            "shuffle":false,
            "drop_last":true,
            "use_shared_memory":false
        },
        "dataset":{
            "data_path":"./data/rvl-cdip/test.txt",
            "image_path":"./data/rvl-cdip/images/",
            "batch_size":1,
            "shuffle":false,
            "transform":[
                {"DecodeImage":{
                    "img_mode":"RGB",
                    "channel_first":false}
                },
                {"NormalizeImage":{
                    "scale":"1./255",
                    "mean":[0.485, 0.456, 0.406],
                    "std":[0.229, 0.224, 0.225],
                    "order":"hwc"}
                },
                {"DetResizeForTest":{
                    "image_pad_shape":[1024, 1024]}
                },
                {"ToCHWImage":{}}
            ]
        }
    }
}
